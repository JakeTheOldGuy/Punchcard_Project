DROP TABLE IF EXISTS PunchRecord;
DROP TABLE IF EXISTS AbsenceRecord;
DROP TABLE IF EXISTS MilesLog;
DROP TABLE IF EXISTS WeekLog;
DROP TABLE IF EXISTS ChangeLog;
DROP TABLE IF EXISTS Employee;



-- Recreate the Employee table
CREATE TABLE Employee (
    employee_id INTEGER PRIMARY KEY,
    first_name TEXT,
    last_name TEXT,
    username TEXT,
    password_hash TEXT,
    four_digit_code TEXT UNIQUE,
    employee_role TEXT
);

-- Recreate the dependent tables
CREATE TABLE PunchRecord (
    punch_id TEXT PRIMARY KEY,
    employee_id INTEGER,
    week_id TEXT,
    record_date DATE,
    punch_in_time TIME,
    punch_out_time TIME,
    break_start_time TIME,
    break_end_time TIME,
    FOREIGN KEY (employee_id) REFERENCES Employee (employee_id)
);

CREATE TABLE AbsenceRecord (
    absence_id TEXT PRIMARY KEY,
    employee_id INTEGER,
    absence_type TEXT,
    absence_date DATE,
    notes TEXT,
    FOREIGN KEY (employee_id) REFERENCES Employee (employee_id)
);

CREATE TABLE MilesLog (
    miles_id TEXT PRIMARY KEY,
    employee_id INTEGER,
    week_id TEXT,
    miles_date DATE,
    miles_driven REAL,
    FOREIGN KEY (employee_id) REFERENCES Employee (employee_id)
);

CREATE TABLE WeekLog (
    week_id TEXT PRIMARY KEY,
    employee_id INTEGER,
    week_ending_day DATE,
    total_hours REAL,
    overtime REAL,
    weighted_hours REAL,
    FOREIGN KEY (employee_id) REFERENCES Employee (employee_id)
);

CREATE TABLE ChangeLog (
    change_id INTEGER PRIMARY KEY AUTOINCREMENT,
    employee_id INTEGER,
    change_type TEXT,
    old_value TEXT,
    new_value TEXT,
    change_date DATE,
    change_time TIME,
    changed_by TEXT,
    FOREIGN KEY (employee_id) REFERENCES Employee (employee_id)
);


